<ul>
<%= form_for @user, url: wizard_path, method: :put do |f| %>
  <%= f.fields_for :skills, @skills do |s| %>
    <div>
      <%= label :name, s.object.subject.name  %>
      <%= s.text_field  :name %>
      <%= s.hidden_field :subject_id %>
    </div>
  <% end %>

  <%= f.submit "Submit" %>
<% end %>
</ul>
